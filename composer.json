{
    "name": "tamasimrei/add-rest",
    "description": "A simple, but extendable RESTful web application",
    "version": "0.0.4",
    "require": {
        "php": ">=7.4",
        "psr/http-message": "^1.0",
        "ext-sqlite3": "*",
        "ext-pdo_sqlite": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*",
        "phpmd/phpmd": "*",
        "sebastian/phpcpd": "*",
        "pdepend/pdepend": "*",
        "phploc/phploc": "*",
        "phpspec/phpspec": "^7.2"
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.4.29"
        }
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Imrei\\": "src/"
        }
    }
}
