{
    "name": "tamasimrei/add-rest",
    "description": "A simple, but extendable RESTful web application",
    "version": "0.0.4",
    "require": {
        "php": ">=5.5.0",
        "psr/http-message": "^1.0",
        "ext-sqlite3": "*",
        "ext-pdo_sqlite": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "squizlabs/php_codesniffer": "^2.5.1",
        "phpmd/phpmd": "^2.4.2",
        "sebastian/phpcpd": "^2.0.1",
        "pdepend/pdepend": "^2.0.3",
        "phploc/phploc": "^2.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Imrei\\": "src/"
        }
    }
}
